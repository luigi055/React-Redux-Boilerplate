{
  "name": "react-redux-boilerplate",
  "version": "1.0.0",
  "description": "Webpack 4 Boilerplate for React + Redux",
  "main": "server.js",
  "scripts": {
    "clear": "rm -rf ./public",
    "start": "npm run clear && export NODE_ENV=production || SET \"NODE_ENV=production\" && webpack --mode production --optimize-minimize && node server.js",
    "dev": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --mode development",
    "build:dev": "rm -rf ./public && webpack",
    "build:dev--watch": "webpack -d -w",
    "build": "export NODE_ENV=production || SET \"NODE_ENV=production\" && rm -rf ./public && webpack -p",
    "lint": "eslint **/*.{js, jsx} --quiet",
    "format": "prettier --write --single-quote --print-width=120 --parser=flow --tab-width=2 \"src/**/*.{js, jsx}\"",
    "test": "export NODE_ENV=test || SET \"NODE_ENV=test\" && mocha ./src/__test__/**/*.test.jsx --require babel-register",
    "test:watch": "yarn run test --watch",
    "flow:init": "flow init",
    "flow:status": "flow status",
    "flow:start": "flow start",
    "flow:stop": "flow stop",
    "flow:coverage": "flow coverage",
    "flow:all": "flow check --all"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:luigi055/react-redux-boilerplate.git"
  },
  "keywords": [
    "react",
    "redux",
    "react-router-v4",
    "webpack4",
    "boilerplate"
  ],
  "author": "Pedro La Rosa",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "8.2.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.2",
    "babel-loader": "7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "chai": "4.1.2",
    "css-loader": "0.28.11",
    "css-mqpacker": "6.0.2",
    "enzyme": "3.3.0",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-config-react": "^1.1.7",
    "eslint-loader": "2.0.0",
    "eslint-plugin-flow": "^2.29.1",
    "eslint-plugin-flowtype": "2.46.1",
    "eslint-plugin-import": "2.9.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-prettier": "2.6.0",
    "eslint-plugin-react": "7.7.0",
    "extract-text-webpack-plugin": "4.0.0-alpha.0",
    "file-loader": "1.1.11",
    "flow-bin": "0.69.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "^3.1.0",
    "image-webpack-loader": "4.2.0",
    "mocha": "5.0.5",
    "node-sass": "4.8.3",
    "pixrem": "4.0.1",
    "postcss-loader": "2.1.3",
    "postcss-will-change": "2.0.0",
    "prettier": "1.11.1",
    "react-hot-loader": "4.0.0",
    "react-test-renderer": "16.3.0",
    "resolve-url-loader": "2.3.0",
    "sass-loader": "6.0.7",
    "script-loader": "0.7.2",
    "sinon": "4.4.9",
    "sinon-chai": "3.0.0",
    "static-files-loader": "0.1.1",
    "static-loader": "^0.1.8",
    "style-loader": "0.20.3",
    "stylelint": "9.1.3",
    "stylelint-config-standard": "18.2.0",
    "url-loader": "1.0.1",
    "webpack": "4.4.1",
    "webpack-cli": "2.0.13",
    "webpack-dev-server": "3.1.1"
  },
  "dependencies": {
    "enzyme-adapter-react-16": "1.1.1",
    "express": "4.16.3",
    "loader-utils": "^1.1.0",
    "react": "16.3.0",
    "react-dom": "16.3.0",
    "react-redux": "5.0.7",
    "react-router-dom": "4.2.2",
    "redux": "^3.6.0",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.2.0"
  }
}
