{
  "name": "react-redux-boilerplate",
  "version": "1.0.0",
  "description": "Webpack 4 Boilerplate for React + Redux",
  "main": "server.js",
  "scripts": {
    "start": "npx webpack --mode production --optimize-minimize && node server.js",
    "dev": "npx webpack-dev-server --mode development",
    "build": "npx webpack --mode production --optimize-minimize",
    "build:dev--watch": "npx webpack --mode development -w",
    "lint": "npx eslint --ext .js,.jsx src/** --quiet",
    "format": "npx prettier --write --single-quote --print-width=120 --parser=flow --tab-width=2 \"src/**/*.{js, jsx}\"",
    "test": "cross-env NODE_ENV=test && npx jest --env=jsdom --config=jest.config.json",
    "test:watch": "npm run test -- --watchAll",
    "test:coverage": "npm run test -- --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:luigi055/react-redux-boilerplate.git"
  },
  "keywords": [
    "react",
    "redux",
    "react-router-v5",
    "webpack4",
    "boilerplate"
  ],
  "author": "Pedro La Rosa",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.4",
    "@babel/plugin-proposal-class-properties": "7.4.4",
    "@babel/plugin-transform-modules-commonjs": "7.4.4",
    "@babel/plugin-transform-runtime": "7.4.4",
    "@babel/preset-env": "7.4.4",
    "@babel/preset-react": "7.0.0",
    "@babel/register": "7.4.4",
    "autoprefixer": "9.5.1",
    "babel-eslint": "10.0.1",
    "babel-jest": "24.7.1",
    "babel-loader": "8.0.5",
    "babel-polyfill": "6.26.0",
    "clean-webpack-plugin": "2.0.1",
    "cross-env": "5.2.0",
    "css-loader": "2.1.1",
    "css-mqpacker": "7.0.0",
    "enzyme": "3.9.0",
    "enzyme-adapter-react-16": "1.12.1",
    "enzyme-to-json": "3.3.5",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "4.2.0",
    "eslint-config-react": "1.1.7",
    "eslint-loader": "2.1.2",
    "eslint-plugin-import": "2.17.2",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.13.0",
    "extract-text-webpack-plugin": "4.0.0-alpha.0",
    "file-loader": "3.0.1",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "image-webpack-loader": "4.6.0",
    "jest": "24.7.1",
    "jsdom": "15.0.0",
    "pixrem": "5.0.0",
    "postcss-loader": "3.0.0",
    "postcss-will-change": "3.0.0",
    "prettier": "1.17.0",
    "raf": "3.4.0",
    "react-hot-loader": "4.8.4",
    "react-test-renderer": "16.8.6",
    "resolve-url-loader": "3.1.0",
    "script-loader": "0.7.2",
    "static-files-loader": "0.1.1",
    "static-loader": "0.1.8",
    "style-loader": "0.23.1",
    "url-loader": "1.1.2",
    "webpack": "4.30.0",
    "webpack-cli": "3.3.2",
    "webpack-dev-server": "3.3.1",
    "webpack-manifest-plugin": "2.0.3"
  },
  "dependencies": {
    "express": "4.16.4",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-redux": "7.0.3",
    "react-router-dom": "5.0.0",
    "redux": "4.0.1",
    "redux-thunk": "2.3.0"
  }
}
